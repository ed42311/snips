   fetchSomeData: function ( req, res ) {

    var = someURL
    var = someAPIReq

    axios.get(someURL + someAPIReq)
     .then(function (response) {
       res.json(response);
       console.log(response;
     })

     .catch(function (response) {
       console.log(response);
     });
   }
}
